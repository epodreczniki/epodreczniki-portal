		<xs:annotation>
			<xs:documentation>Opcje dla URL licencji WOMI/cytatu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value=""/>
			<xs:enumeration value="http://www.epodreczniki.pl/licenses/e-podreczniki/1.0"/>
			<xs:enumeration value="http://creativecommons.org/publicdomain/zero/1.0/legalcode"/>
