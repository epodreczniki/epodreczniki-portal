			<xs:documentation>Wzór wstawiania bibliografii</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="title">
				<xs:complexType/>
			</xs:element>
			<xs:element name="author">
				<xs:complexType/>
			</xs:element>
			<xs:element name="booktitle">
				<xs:complexType/>
			</xs:element>
			<xs:element name="year">
				<xs:complexType/>
			</xs:element>
			<xs:element name="key">
				<xs:complexType/>
			</xs:element>
			<xs:element name="edition">
				<xs:complexType/>
			</xs:element>
			<xs:element name="organization">
				<xs:complexType/>
			</xs:element>
			<xs:element name="address">
				<xs:complexType/>
			</xs:element>
			<xs:element name="series">
				<xs:complexType/>
			</xs:element>
			<xs:element name="editor">
				<xs:complexType/>
			</xs:element>
			<xs:element name="publisher">
				<xs:complexType/>
			</xs:element>
			<xs:element name="number">
				<xs:complexType/>
			</xs:element>
			<xs:element name="journal">
				<xs:complexType/>
			</xs:element>
			<xs:element name="note">
				<xs:complexType/>
			</xs:element>
			<xs:element name="howpublished">
				<xs:complexType/>
			</xs:element>
			<xs:element name="pages">
				<xs:complexType/>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:element name="bibliographyPattern" type="ep:bibliographyPatternType">
		<xs:annotation>
			<xs:documentation>Element wzóru wstawiania bibliografii.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:attribute name="style">
		<xs:annotation>
			<xs:documentation>atrybut styl dla cn:table</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="zebra"></xs:enumeration>
				<xs:enumeration value="plain"></xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:attribute>
	<xs:attribute name="collection-version" type="xs:positiveInteger">
		<xs:annotation>
			<xs:documentation>Atrybut zawierający wersję kolekcji w łączu</xs:documentation>
		</xs:annotation>
	</xs:attribute>
	<xs:attribute name="collection-id" type="ep:idType">
		<xs:annotation>
			<xs:documentation>Atrybut zawierający id kolekcji w łączu</xs:documentation>
		</xs:annotation>
	</xs:attribute>
	<xs:attribute name="collection-recipient" type="xs:string">
		<xs:annotation>
			<xs:documentation>Atrybut zawierający odbiorcę treści kolekcji w łączu</xs:documentation>
		</xs:annotation>
	</xs:attribute>
</xs:schema>
