			<xs:enumeration value="https://creativecommons.org/licenses/by-nc-nd/4.0/legalcode"/>
			<xs:enumeration value="https://creativecommons.org/licenses/by-nc-sa/1.0/legalcode"/>
			<xs:enumeration value="https://creativecommons.org/licenses/by-nc-sa/2.0/pl/legalcode"/>
